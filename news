  $(document).ready(function(){
    $(window).scroll(function(){
      var scroll = $(window).scrollTop();
      if(scroll>10){
        $(".top-nav").addClass("lk-nav");
      }
      else{
        $(".top-nav").removeClass("lk-nav");
      }
    });
});


